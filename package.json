{
  "name": "@uploadcare/upload-client",
  "version": "1.1.5",
  "description": "Library for work with Uploadcare Upload API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "browser": {
    "./lib/request/request.node.js": "./lib/request/request.browser.js",
    "./lib/tools/getFormData.node.js": "./lib/tools/getFormData.browser.js",
    "./lib/tools/sockets.node.js": "./lib/tools/sockets.browser.js"
  },
  "react-native": {
    "./lib/request/request.node.js": "./lib/request/request.browser.js",
    "./lib/tools/getFormData.node.js": "./lib/tools/getFormData.react-native.js",
    "./lib/tools/sockets.node.js": "./lib/tools/sockets.browser.js",
    "./lib/uploadFile/prepareChunks.node.js": "./lib/uploadFile/prepareChunks.react-native.js"
  },
  "scripts": {
    "check-env-vars": "node ./checkvars.js",
    "mock:start": "ts-node --project ./mock-server/tsconfig.mock.json ./mock-server/server.ts --silent",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "test": "start-server-and-test mock:start :3000 test:jest",
    "test:production": "npm run check-env-vars && TEST_ENV=production jest",
    "test:jest": "jest",
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.build.json",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-upload-client.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-upload-client/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-upload-client#readme",
  "keywords": [
    "uploadcare",
    "file",
    "uploader",
    "image",
    "upload",
    "store",
    "cdn"
  ],
  "devDependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "10.0.0",
    "@types/form-data": "2.5.0",
    "@types/jest": "26.0.24",
    "@types/koa": "2.13.4",
    "@types/node": "12.20.16",
    "@types/promise": "7.1.30",
    "@types/ws": "7.4.6",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "chalk": "4.1.1",
    "data-uri-to-buffer": "3.0.1",
    "dataurl-to-blob": "0.0.1",
    "dotenv": "8.6.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "jest-environment-jsdom": "26.6.2",
    "jest-websocket-mock": "2.2.1",
    "koa": "2.13.1",
    "koa-add-trailing-slashes": "2.0.1",
    "koa-body": "4.2.0",
    "mock-socket": "9.0.3",
    "prettier": "2.3.2",
    "prettier-config-standard": "4.0.0",
    "rimraf": "3.0.2",
    "shipjs": "0.23.3",
    "start-server-and-test": "1.12.6",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "3.9.10"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "ws": "^7.2.1"
  }
}
